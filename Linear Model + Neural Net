    "def updateCoeffs(coeffs, learningRate):\n",
    "    coeffs.sub_(coeffs.grad * learningRate)\n",
    "    coeffs.grad.zero_()\n",
    "    \n",
    "def initCoeffecients():\n",
    "    return (torch.rand(numCoeffecients)-0.5).requires_grad_()\n"
   ]
